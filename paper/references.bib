doi = {10.1016/j.neuroimage.2008.12.037},
file = {:C$\backslash$:/Users/Jes\'{u}sOmar/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Klein et al. - 2009 - Evaluation of 14 nonlinear deformation algorithms applied to human brain MRI registration.pdf:pdf},
